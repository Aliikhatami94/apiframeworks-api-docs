{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "declarationDir": "dist",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "emitDeclarationOnly": false,
    "noEmit": false
  },
  "include": ["src"]
}